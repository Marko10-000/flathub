{
  "id": "org.zealdocs.Zeal",
  "runtime": "org.kde.Platform",
  "runtime-version": "5.11",
  "sdk": "org.kde.Sdk",
  "base": "io.qt.qtwebkit.BaseApp",
  "base-version": "5.11",
  "command": "zeal",
  "rename-icon": "zeal",
  "finish-args": [
    "--socket=wayland",
    "--socket=x11",
    "--share=ipc",
    "--device=dri",
    "--share=network"
  ],
  "cleanup": [
    "/mkspecs",
    "/include",
    "/lib/cmake",
    "/lib/mkspecs"
  ],
  "modules": [
    {
      "name": "zeal",
      "buildsystem": "cmake-ninja",
      "config-opts": [
        "-DCMAKE_BUILD_TYPE=RelWithDebInfo",
        "-DQt5WebKit_DIR=/app/lib/cmake/Qt5WebKit",
        "-DQt5WebKitWidgets_DIR=/app/lib/cmake/Qt5WebKitWidgets"
      ],
      "sources": [
        {
          "type": "archive",
          "url": "https://github.com/zealdocs/zeal/archive/v0.6.1.tar.gz",
          "sha256": "095c08f9903071849d5c79878abd48237ce1615f16d324afff1873ab6b5f0026"
        }
      ]
    }
  ]
}
